<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.TabbedViewNavigator")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="normal" />
	</s:states>
	
	<!-- SkinParts
	name=contentGroup, type=spark.components.Group, required=false
	name=tabBar, type=spark.components.supportClasses.ButtonBarBase, required=false
	-->
	
	<s:BitmapImage source="@Embed('../assets/images/bottom-bg.PNG')" includeIn="disabled"/>
	<s:BitmapImage source="@Embed('../assets/images/bottom-bg.PNG')" includeIn="normal"/>
	
</s:Skin>
